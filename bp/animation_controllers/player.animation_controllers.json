{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.dash": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "transitions": [
                        { "jump_one": "q.is_jumping" }
                    ]
                },
                "jump_one": {
                    "transitions": [
                        { "idle": "q.is_on_ground" },
                        { "released_jump_key": "!q.is_jumping" }
                    ]
                },
                "released_jump_key": {
                    "transitions": [
                        { "idle": "q.is_on_ground" },
                        { "launch": "q.is_sneaking" }
                    ]
                },
                "launch": {
                    "on_entry": [
                        "/summon 20w14s:launcher ~ ~-0.25 ~ 20w14s:dash",
                        "/effect @s speed 3 1 true"
                    ],
                    "transitions": [
                        { "cooldown": "(1.0)" }
                    ]
                },
                "cooldown": {
                    "on_entry": [
                        "/playsound note.bit @s ~ ~ ~ 1 0.35",
                        "v.dash_cooldown = q.time_stamp + 15;"
                    ],
                    "on_exit": [
                        "/playsound note.bit @s ~ ~ ~ 1 1"
                    ],
                    "transitions": [
                        { "idle": "q.is_on_ground && q.time_stamp >= v.dash_cooldown" }
                    ]
                }
            }
        },
        "controller.animation.hurt_by_phantom": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [
                        { "hurt": "q.is_stunned" }
                    ]
                },
                "hurt": {
                    "on_entry": [
                        "/playsound 20w14s.mob.player.jeb_ouch @s ~ ~ ~ 1",
                        "/structure load how_to_sleep ~ ~ ~",
                        "/tellraw @s {\"rawtext\":[{\"text\":\"Â§b[Jeb] Hey kids, Jens 'Jeb' Bergensten here! Make sure to get plenty of sleep to stay healthy, and eat a balanced breakfast, the building blocks of a healthy lifestyle! Have fun and keep crafting! Jeb out\"}]}"
                    ],
                    "transitions": [
                        { "default": "(1.0)" }
                    ]
                }
            }
        }
    }
}