{
    "format_version": "1.17.10",
    "minecraft:entity": {
        "description": {
            "identifier": "minecraft:player",
            "is_spawnable": false,
            "is_summonable": false,
            "is_experimental": false,
            "animations": {
                "dash": "controller.animation.dash",
                "hurt_by_phantom": "controller.animation.hurt_by_phantom",
                "melon_uzi": "controller.animation.melon_uzi.use",
                "grappling_hook": "controller.animation.grappling_hook.use",
                "running_shoes_controller": "controller.animation.running_shoes",
                "running_level_1": "animation.running_shoes.level_1",
                "running_level_2": "animation.running_shoes.level_2",
                "running_level_3": "animation.running_shoes.level_3",
                "running_level_4": "animation.running_shoes.level_4",
                "running_level_5": "animation.running_shoes.level_5"
            },
            "scripts": {
                "animate": [
                    "dash",
                    "hurt_by_phantom",
                    "melon_uzi",
                    "grappling_hook",
                    "running_shoes_controller"
                ]
            }
        },
        "component_groups": {
            "test:hurt_by_phantom": {
                "minecraft:is_stunned": {},
                "minecraft:timer": {
                    "time": 0.0,
                    "looping": false,
                    "time_down_event": {
                        "event": "test:reset_hurt_by_phantom",
                        "target": "self"
                    }
                }
            },
            "test:shoot_melon_uzi": {
                "minecraft:spawn_entity": {
                    "entities": [
                        {
                            "spawn_entity": "test:melon_seed",
                            "single_use": true,
                            "min_wait_time": 0,
                            "max_wait_time": 0,
                            "num_to_spawn": 1
                        }
                    ]
                },
                "minecraft:timer": {
                    "looping": false,
                    "time": 0.0,
                    "time_down_event": {
                        "event": "test:reset_melon_uzi",
                        "target": "self"
                    }
                }
            },
            "test:shoot_grappling_hook": {
                "minecraft:is_charged": {},
                "minecraft:spawn_entity": {
                    "entities": [
                        {
                            "spawn_entity": "test:grappling_hook",
                            "spawn_event": "test:init",
                            "single_use": true,
                            "min_wait_time": 0,
                            "max_wait_time": 0,
                            "num_to_spawn": 1
                        }
                    ]
                },
                "minecraft:timer": {
                    "looping": false,
                    "time": 0.5,
                    "time_down_event": {
                        "event": "test:reset_grappling_hook",
                        "target": "self"
                    }
                }
            },
            "minecraft:add_bad_omen": {
                "minecraft:spell_effects": {
                    "add_effects": [
                        {
                            "effect": "bad_omen",
                            "duration": 6000,
                            "display_on_screen_animation": true
                        }
                    ]
                },
                "minecraft:timer": {
                    "time": [
                        0.0,
                        0.0
                    ],
                    "looping": false,
                    "time_down_event": {
                        "event": "minecraft:clear_add_bad_omen",
                        "target": "self"
                    }
                }
            },
            "minecraft:clear_bad_omen_spell_effect": {
                "minecraft:spell_effects": {}
            },
            "minecraft:raid_trigger": {
                "minecraft:raid_trigger": {
                    "triggered_event": {
                        "event": "minecraft:remove_raid_trigger",
                        "target": "self"
                    }
                },
                "minecraft:spell_effects": {
                    "remove_effects": "bad_omen"
                }
            }
        },
        "components": {
            "minecraft:experience_reward": {
                "on_death": "Math.Min(query.player_level * 7, 100)"
            },
            "minecraft:type_family": {
                "family": [
                    "player"
                ]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:loot": {
                "table": "loot_tables/empty.json"
            },
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 1.8
            },
            "minecraft:can_climb": {},
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self",
                            "operator": "==",
                            "value": true
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "cause": "entity_attack",
                        "deals_damage": true,
                        "on_damage": {
                            "filters": {
                                "all_of": [
                                    { "test": "is_family", "subject": "other", "operator": "==", "value": "phantom" }
                                ]
                            },
                            "target": "self",
                            "event": "test:on_hurt_by_phantom"
                        }
                    }
                ]
            },
            "minecraft:attack": {
                "damage": 1
            },
            "minecraft:player.saturation": {
                "value": 20
            },
            "minecraft:player.exhaustion": {
                "value": 0,
                "max": 4
            },
            "minecraft:player.level": {
                "value": 0,
                "max": 24791
            },
            "minecraft:player.experience": {
                "value": 0,
                "max": 1
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": -1,
                "inhale_time": 3.75,
                "generates_bubbles": false
            },
            "minecraft:nameable": {
                "always_show": true,
                "allow_name_tag_renaming": false
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": false,
                "is_pushable_by_piston": true
            },
            "minecraft:insomnia": {
                "days_until_insomnia": 3
            },
            "minecraft:rideable": {
                "seat_count": 2,
                "family_types": [
                    "parrot_tame"
                ],
                "pull_in_entities": true,
                "seats": [
                    {
                        "position": [
                            0.4,
                            -0.2,
                            -0.1
                        ],
                        "min_rider_count": 0,
                        "max_rider_count": 0,
                        "lock_rider_rotation": 0
                    },
                    {
                        "position": [
                            -0.4,
                            -0.2,
                            -0.1
                        ],
                        "min_rider_count": 1,
                        "max_rider_count": 2,
                        "lock_rider_rotation": 0
                    }
                ]
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:block_climber": {},
            "minecraft:environment_sensor": {
                "triggers": {
                    "filters": {
                        "all_of": [
                            {
                                "test": "has_mob_effect",
                                "subject": "self",
                                "value": "bad_omen"
                            },
                            {
                                "test": "is_in_village",
                                "subject": "self",
                                "value": true
                            }
                        ]
                    },
                    "event": "minecraft:trigger_raid"
                }
            }
        },
        "events": {
            "minecraft:gain_bad_omen": {
                "add": {
                    "component_groups": [
                        "minecraft:add_bad_omen"
                    ]
                }
            },
            "minecraft:clear_add_bad_omen": {
                "remove": {
                    "component_groups": [
                        "minecraft:add_bad_omen"
                    ]
                },
                "add": {
                    "component_groups": [
                        "minecraft:clear_bad_omen_spell_effect"
                    ]
                }
            },
            "minecraft:trigger_raid": {
                "add": {
                    "component_groups": [
                        "minecraft:raid_trigger"
                    ]
                }
            },
            "minecraft:remove_raid_trigger": {
                "remove": {
                    "component_groups": [
                        "minecraft:raid_trigger"
                    ]
                }
            },
            "test:on_hurt_by_phantom": {
                "add": {
                    "component_groups": [
                        "test:hurt_by_phantom"
                    ]
                }
            },
            "test:reset_hurt_by_phantom": {
                "remove": {
                    "component_groups": [
                        "test:hurt_by_phantom"
                    ]
                }
            },
            "test:on_shoot_melon_uzi": {
                "add": {
                    "component_groups": [
                        "test:shoot_melon_uzi"
                    ]
                }
            },
            "test:reset_melon_uzi": {
                "remove": {
                    "component_groups": [
                        "test:shoot_melon_uzi"
                    ]
                }
            },
            "test:on_shoot_grappling_hook": {
                "add": {
                    "component_groups": [
                        "test:shoot_grappling_hook"
                    ]
                }
            },
            "test:reset_grappling_hook": {
                "remove": {
                    "component_groups": [
                        "test:shoot_grappling_hook"
                    ]
                }
            }
        }
    }
}